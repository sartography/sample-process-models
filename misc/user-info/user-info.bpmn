      <bpmn:outgoing>Flow_05s4pvl</bpmn:outgoing>
      <bpmn:script># If not previously set, set find_user to current_user
try:
  find_user
except NameError:
  find_user = "harmeet@status.im"
  isCurrentUser = True

# Check if find_user has a Status email
is_status_email = (find_user[-9:] == "status.im")

# current_user["username"]
process_info = {}
process_info["process_model_identifier"] = "manage-procurement/procurement/requisition-order-management/raise-new-demand-request"</bpmn:script>
    </bpmn:scriptTask>
