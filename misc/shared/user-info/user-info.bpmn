current_user_extras["bamboo_supervisor_email"] = userBambooId = resp_BambooUserInfo["supervisorEmail"]
current_user_extras["bamboo_supervisor_status_id"] = userBambooId = resp_BambooUserInfo["supervisorId"]</bpmn:script>
